{
    "routes": [
        {
            "route": "/signin",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/signup",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/chat",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "responseOverrides": {
        "400": {
            "rewrite": "/"
        },
        "401": {
            "redirect": "/"
        },
        "403": {
            "rewrite": "/"
        },
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "mimeTypes": {
        ".html": "text/html",
        ".json": "text/json",
        ".css": "text/css",
        ".js": "text/javascript"
    }
  
  }